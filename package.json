{
  "name": "workout",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "if-env TYPE=server && npm run build-server || npm run build-client",
    "build-server": "cd server && npm install && npm run build",
    "build-client": "cd client && npm install && npm run build",
    "start": "npm run start-client-server",
    "start-client-server": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "start-client": "cd client && npm run start",
    "start-server": "cd server && npm run start",
    "server": "node ./server/app.jsx"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "if-env": "^1.0.4"
  }
}
